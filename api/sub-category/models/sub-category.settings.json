{
  "kind": "collectionType",
  "collectionName": "sub_categories",
  "info": {
    "name": "sub_category"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sub_category_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "category_name": {
      "type": "string"
    },
    "category_image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categories": {
      "collection": "categories",
      "via": "sub_categories",
      "dominant": true
    },
    "companies": {
      "collection": "companies",
      "via": "sub_categories",
      "dominant": true
    },
    "products": {
      "via": "sub_categories",
      "collection": "products",
      "dominant": true
    }
  }
}
